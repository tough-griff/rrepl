{
  "name": "rrepl",
  "version": "1.0.0",
  "description": "An improved node REPL with support for a .noderc file and global package installs.",
  "keywords": [
    "REPL"
  ],
  "author": "Griffin Yourick <gryphon92@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tough-griff/rrepl.git"
  },
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "lint": "eslint index.js",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "semver": "^6.2.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
